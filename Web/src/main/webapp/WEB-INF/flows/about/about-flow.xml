<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	  parent="abstractBoard,abstractEditable,baseGood">
	<on-start>
		<!--abstractBoard properties-->
		<set name="flowScope.boardType" value="'ABOUT'"/>
		<set name="flowScope.viewName" value="'aboutUs'"/>
		<set name="flowScope.flowName" value="'about'"/>
		<!--baseGood properties-->
		<set name="flowScope.outState" value="'aboutUs'"/>
        <set name="flowScope.selectFlow" value="'good'"/>
        <set name="flowScope.editFlow" value="'editGood'"/>
        <set name="flowScope.className" value="'com.meitan.lubov.model.persistent.Product'"/>
        <set name="conversationScope.dao" value="productDao"/>
		<!--menu background-->
	</on-start>

	<view-state id="aboutUs" parent="abstractBoard#abstractBoardViewState">
		<on-render>
			<set name="flowScope.action" value="utils.getMethodExpression('selectGood')"/>
        </on-render>

        		<transition on="test">
						<evaluate expression="utils.testMessage(messageContext)"/>

		</transition>

	</view-state>

    <end-state id="finish" view="../abstractEditable/conformDelete.xhtml"/>
</flow>