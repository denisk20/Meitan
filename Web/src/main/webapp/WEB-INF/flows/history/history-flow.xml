<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
		parent="baseGood,abstractEditable"
		start-state="allCategoriesQueried">

	<secured attributes="ROLE_CLIENT, ROLE_CONSULTANT, ROLE_ADMIN" match="any"/>
	
	<view-state id="history">
		<on-entry>
			<evaluate expression="buyingActDao.findByLogin(currentUser.name)" result="boughts"/>
		</on-entry>
	</view-state>
</flow>